{
  "expo": {
    "name": "fumedeme-expo-widget-example",
    "slug": "fumedeme-expo-widget-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.fumedemeexpowidget.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "expo.modules.fumedemeexpowidget.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.0",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildFeatures": {
              "compose": true
            },
            "composeCompilerVersion": "1.5.15",
            "kotlinOptions": {
              "jvmTarget": "1.8"
            }
          }
        }
      ],
      [
        "./../app.plugin.js",
        [
          {
            "widgetName": "DenemeIstasyonWidget",
            "ios": {
              "devTeamId": "YOUR_TEAM_ID",
              "appGroupIdentifier": "group.expo.modules.istasyondeneme.example"
            }
          },
          {
            "widgetName": "DenemeHavaWidget",
            "ios": {
              "devTeamId": "YOUR_TEAM_ID",
              "appGroupIdentifier": "group.expo.modules.havadeneme.example"
            }
          }
        ]
      ]
    ]
  }
}
