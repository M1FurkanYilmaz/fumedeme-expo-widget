{
  "expo": {
    "name": "fumedeme-expo-widget-example",
    "slug": "fumedeme-expo-widget-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.fumedemeexpowidget.example",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.expo.modules.istasyondeneme.example"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "expo.modules.fumedemeexpowidget.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "fumedeme-expo-widget",
        {
          "widgetName": "DenemeWidget",
          "ios": {
            "devTeamId": "YOUR_TEAM_ID",
            "appGroupIdentifier": "group.expo.modules.istasyondeneme.example"
          }
        }
      ]
    ]
  }
}
